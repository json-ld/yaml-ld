%YAML 1.2
%TAG !xsd! http://www.w3.org/2001/XMLSchema%23
---
- http://example.com/yint:
  - "@value": 123
  http://example.com/yfloat:
  - "@value": 123.45
  http://example.com/ystring:
  - "@value": string
  http://example.com/ybool:
  - "@value": true
  http://example.com/integer:
  - "@value": !xsd!integer 123
  http://example.com/decimal:
  - "@value": !xsd!decimal 123.456
  http://example.com/double:
  - "@value": !xsd!double 123.456e78
  http://example.com/bool:
  - "@value": !xsd!boolean true
  http://example.com/date:
  - "@value": !xsd!date 2022-08-08
  http://example.com/time:
  - "@value": !xsd!time 12:00:00.000
  http://example.com/dateTime:
  - "@value": !xsd!dateTime 2022-08-08T12:00:00.000
